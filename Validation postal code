# Enter your code here. Read input from STDIN. Print output to STDOUT
import re
def is_valid(card):
    pattern = r'^[456]\d{3}(-?\d{4}){3}$'
    if not re.match(pattern, card):
        return False
    if re.search(r'[^0-9-]', card):
        return False
    if "--" in card or " " in card or "_" in card:
        return False
    digits = card.replace("-", "")
    if len(digits) != 16:
        return False
    if re.search(r'(.)\1{3,}', digits):
        return False
    return True
n = int(input())
for _ in range(n):
    card = input().strip()
    print("Valid" if is_valid(card) else "Invalid")
